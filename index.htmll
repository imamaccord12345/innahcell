<!DOCTYPE html>
<html lang="id">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
  <title>INNAH CELL — Paket Data</title>
  <meta name="description" content="INNAH CELL: Beli cepat paket XL Circle, Akrab, Flex Mania, dan Masa Aktif via WhatsApp." />
  <meta name="theme-color" content="#0a0f1f" />
  <style>
    :root{
      --bg1:#0a0f1f; --bg2:#0f182e; --text:#eaf0ff; --muted:#a9b3d1;
      --accent:#4fd1c5; --accent2:#8b5cf6; --card:#121a33; --border:rgba(255,255,255,.08);
      --shadow:0 10px 30px rgba(0,0,0,.35); --radius:18px;
    }
    *{box-sizing:border-box}
    html{scroll-behavior:smooth}
    body{
      margin:0; color:var(--text); font-family:ui-sans-serif,system-ui,-apple-system,Segoe UI,Roboto,Arial;
      background: radial-gradient(1200px 600px at 10% 0%, #101a36 0%, var(--bg1) 40%),
                  radial-gradient(900px 400px at 90% 10%, #13224a 0%, var(--bg2) 60%);
      overflow-x:hidden;
    }
    #bg-wrap{position:fixed; inset:0; z-index:-1}
    #bg-chart{width:100%; height:100%; display:block; filter:drop-shadow(0 0 6px rgba(79,209,197,.35)); opacity:.28}

    header{position:sticky; top:0; z-index:20; backdrop-filter:blur(10px);
      background:linear-gradient(180deg, rgba(18,26,51,.92), rgba(18,26,51,.6)); border-bottom:1px solid var(--border)}
    .container{max-width:1000px; margin:0 auto; padding:clamp(12px,3vw,22px)}
    .brand{display:flex; align-items:center; gap:12px}
    .brand h1{margin:0; font-size:clamp(18px,5vw,28px)}
    .wa-chip{margin-left:auto; display:inline-flex; align-items:center; gap:10px; padding:10px 14px;
      border-radius:999px; background:#0b3e2c; color:#bfffe9; text-decoration:none; border:1px solid #1e6249;
      box-shadow:var(--shadow); font-weight:700; white-space:nowrap}
    .wa-chip svg{width:20px; height:20px}

    .card{background:linear-gradient(180deg, rgba(18,26,51,.9), rgba(18,26,51,.78)); border:1px solid var(--border);
      border-radius:var(--radius); box-shadow:var(--shadow); padding:clamp(14px,3vw,22px)}
    .section{margin:clamp(16px,3vw,24px) auto}
    .muted{color:var(--muted)}

    .input-row{display:flex; gap:12px; flex-wrap:wrap; align-items:end}
    label{font-size:.9rem; color:var(--muted)}
    input{font:inherit}
    .text-input{padding:12px 14px; border-radius:12px; border:1px solid rgba(255,255,255,.12);
      background:#0e1530; color:var(--text); min-width:min(260px, 100%); width:100%; outline:none}
    .btn{cursor:pointer; border:none; border-radius:14px; padding:14px 16px; font-weight:800;
      background:linear-gradient(135deg, var(--accent), var(--accent2)); color:white; box-shadow:var(--shadow)}
    .btn:active{transform:translateY(1px)}

    .table-wrap{overflow-x:auto; -webkit-overflow-scrolling:touch}
    table{width:100%; border-collapse:separate; border-spacing:0 10px; min-width:520px}
    th,td{padding:12px 14px; text-align:left}
    thead th{color:#cbd5ff; font-size:.95rem}
    tbody tr{background:#111a34; border:1px solid var(--border)}
    tbody tr td:first-child{border-radius:12px 0 0 12px}
    tbody tr td:last-child{border-radius:0 12px 12px 0}

    /* Cards grid responsive */
    .grid{display:grid; gap:12px}
    .cols-2{grid-template-columns:repeat(2,1fr)}
    .cols-3{grid-template-columns:repeat(3,1fr)}
    @media(max-width:900px){ .cols-2,.cols-3{grid-template-columns:1fr} }

    details{background:#10183a; border:1px solid var(--border); border-radius:14px; padding:12px 14px}
    summary{cursor:pointer; list-style:none; font-weight:800}
    summary::-webkit-details-marker{display:none}
    .pill{display:inline-block; padding:4px 10px; border:1px solid rgba(255,255,255,.2); border-radius:999px; font-size:.8rem; color:#cfe6ff}

    .wa-fab{position:fixed; right:16px; bottom:84px; background:#25d366; color:#053; border:none;
      border-radius:999px; padding:14px 18px; box-shadow:var(--shadow); font-weight:900; display:flex; align-items:center; gap:10px; z-index:30}
    .wa-fab svg{width:22px; height:22px}

    .bottom-nav{position:fixed; left:0; right:0; bottom:0; z-index:25; background:rgba(12,18,40,.95);
      border-top:1px solid var(--border); backdrop-filter:blur(10px)}
    .nav-inner{max-width:1000px; margin:0 auto; display:grid; grid-template-columns:repeat(4,1fr); gap:8px; padding:8px 12px}
    .nav-inner a{display:flex; justify-content:center; align-items:center; padding:12px; border-radius:12px; text-decoration:none; color:#cfe6ff;
      border:1px solid var(--border); background:#111a34; font-weight:700; font-size:.95rem}

    footer{opacity:.8; font-size:.9rem; padding:64px 0 100px; text-align:center}
    @media(min-width:900px){ .wa-fab{bottom:24px} footer{padding-bottom:60px} }
  </style>
</head>
<body>
  <div id="bg-wrap"><canvas id="bg-chart"></canvas></div>

  <header>
    <div class="container brand">
      <h1>INNAH CELL</h1>
      <a class="wa-chip" id="waTop" target="_blank" rel="noopener" href="#">WhatsApp: 0823-0139-2317</a>
    </div>
  </header>

  <main class="container">
    <section class="section card">
      <h2>Menu Paket & Pembelian Cepat</h2>
      <p class="muted">Isi <b>Nomor Tujuan</b>, lalu klik <b>Beli via WhatsApp</b> di paket yang dipilih. Pesan otomatis terisi.</p>
      <div class="input-row" style="margin-top:10px">
        <div style="flex:1 1 240px">
          <label for="custName">Nama (opsional)</label><br>
          <input id="custName" class="text-input" placeholder="Nama Anda (mis. Budi)" inputmode="text" autocomplete="name" />
        </div>
        <div style="flex:1 1 240px">
          <label for="custNumber">Nomor Tujuan</label><br>
          <input id="custNumber" class="text-input" placeholder="08xxxxxxxxxx" inputmode="tel" autocomplete="tel" />
        </div>
        <button class="btn" onclick="testChat()">Tes Chat ke Admin</button>
      </div>
    </section>

    <!-- XL CIRCLE -->
    <section id="circle" class="section">
      <div class="card">
        <h2>MENU PAKET DATA XL CIRCLE</h2>
        <ul class="muted">
          <li>Nomor aktif ≥ 60 hari & belum jadi anggota Circle bulan berjalan.</li>
          <li>Tidak menambah masa aktif (masa aktif paket 28 hari).</li>
          <li>Kesalahan nomor bukan tanggung jawab kami.</li>
        </ul>
        <div class="table-wrap">
          <table>
            <thead><tr><th>PAKET</th><th>HARGA</th><th></th></tr></thead>
            <tbody id="tbCircle"></tbody>
          </table>
        </div>
      </div>
    </section>

    <!-- AKRAB -->
    <section id="akrab" class="section">
      <div class="card">
        <h2>MENU PAKET AKRAB</h2>
        <ul class="muted">
          <li>Bisa untuk XL / AXIS / LIVE ON.</li>
          <li>Resmi & bergaransi.</li>
          <li>Jika myReward tidak masuk, tunggu 1 jam lalu hubungi admin.</li>
        </ul>
        <div class="grid cols-2" id="akrabList"></div>
      </div>
    </section>

    <!-- FLEX MANIA -->
    <section id="flex" class="section">
      <div class="card">
        <h2>MENU PAKET FLEX MANIA — Claim Bonus</h2>
        <div class="grid cols-2" id="flexList"></div>
      </div>
    </section>

    <!-- MASA AKTIF -->
    <section id="masaaktif" class="section">
      <div class="card">
        <h2>MENU MASA AKTIF 1 BULAN</h2>
        <p class="muted">Pastikan kirim nomor dengan benar.</p>
        <div class="grid cols-3" id="masaAktif"></div>
      </div>
    </section>

    <button class="wa-fab" onclick="openWA('Halo admin, saya ingin bertanya seputar paket.');" aria-label="Chat Admin WhatsApp">
      <svg viewBox="0 0 32 32" fill="currentColor" aria-hidden="true"><path d="M19.11 17.62c-.27-.13-1.6-.78-1.84-.87-.25-.09-.43-.13-.6.13-.18.27-.69.86-.85 1.04-.16.18-.31.2-.58.07-.27-.13-1.12-.41-2.13-1.31-.79-.7-1.32-1.55-1.48-1.81-.15-.27-.02-.41.11-.54.11-.11.27-.29.4-.44.13-.16.18-.27.27-.45.09-.18.04-.34-.02-.47-.07-.13-.6-1.45-.82-1.99-.22-.53-.45-.46-.6-.46-.16 0-.34-.01-.52-.01-.18 0-.47.07-.72.34-.25.27-.95.93-.95 2.26 0 1.33.98 2.62 1.12 2.8.13.18 1.93 2.94 4.68 4.01.65.28 1.16.45 1.56.57.65.2 1.25.17 1.72.1.53-.08 1.6-.65 1.83-1.27.23-.62.23-1.15.16-1.27-.07-.11-.25-.18-.52-.31z"/><path d="M26.88 5.12C24.05 2.3 20.27.8 16.29.8 8.4.8 2 7.2 2 15.1c0 2.51.66 4.96 1.92 7.11L2 31.2l9.19-1.88c2.09 1.14 4.46 1.74 6.9 1.74h0c7.89 0 14.29-6.4 14.29-14.29 0-3.99-1.55-7.77-4.38-10.6zM16.09 28.2h0c-2.24 0-4.43-.6-6.35-1.74l-.46-.27-5.46 1.12 1.15-5.33-.3-.55C3.48 19.3 2.8 17.23 2.8 15.1c0-7.4 6.01-13.41 13.41-13.41 3.58 0 6.94 1.4 9.46 3.92 2.52 2.52 3.92 5.88 3.92 9.46 0 7.4-6.01 13.41-13.41 13.41z"/></svg>
      Chat Admin
    </button>

    <nav class="bottom-nav" aria-label="Navigasi Cepat">
      <div class="nav-inner">
        <a href="#circle">XL Circle</a>
        <a href="#akrab">Akrab</a>
        <a href="#flex">Flex Mania</a>
        <a href="#masaaktif">Masa Aktif</a>
      </div>
    </nav>

    <footer>© <span id="year"></span> INNAH CELL • Beli paket cepat & aman via WhatsApp</footer>
  </main>

  <script>
    const ADMIN_WA_NUMBER = '6282301392317';

    const circlePackages = [
      {name:'XL Circle 7-11GB', price:28000},
      {name:'XL Circle 17-21GB', price:37000},
      {name:'XL Circle 27-31GB', price:46000},
      {name:'XL Circle 32-36GB', price:50000},
      {name:'XL Circle 37-41GB', price:55000},
      {name:'XL Circle 42-46GB', price:60000},
      {name:'XL Circle 47-51GB', price:65000},
      {name:'XL Circle 57-61GB', price:75000},
      {name:'XL Circle 62-66GB', price:80000},
      {name:'XL Circle 72-76GB', price:87000},
      {name:'XL Circle 77-81GB', price:93000},
      {name:'XL Circle 82-86GB', price:95000},
      {name:'XL Circle 87-91GB', price:100000},
      {name:'XL Circle 97-101GB', price:110000},
      {name:'XL Circle 107-111GB', price:120000},
    ];

    const akrabPackages = [
      {name:'AKRAB 90GB', price:100000},
      {name:'AKRAB 52GB', price:75000},
      {name:'AKRAB 49GB', price:70000},
      {name:'AKRAB 40GB', price:65000},
      {name:'AKRAB 33GB', price:60000},
    ];

    const flexPackages = [
      {name:'FLEX MANIA 6GB', price:35000, details:[
        'Kuota Utama 6GB Nasional',
        'Bisa diakumulasi dengan jenis/kode paket yang sama',
        'Bonus: Reguler 1GB / YouTube & TikTok 1.5GB / Kuota Malam 2GB (via MyXL/*848#)',
        'Tidak boleh ada jenis paket Flex lain (Unreg jika ada)',
        'Masa aktif 28 hari • Hanya XL'
      ]},
      {name:'FLEX MANIA 10GB', price:39000, details:[
        'Bisa diakumulasi dengan jenis/kode paket yang sama',
        'Bonus: Reguler 2GB / YouTube & TikTok 3GB / Kuota Malam 4GB (via MyXL/*848#)',
        'Tidak boleh ada jenis paket Flex lain (Unreg jika ada)',
        'Masa aktif 28 hari • Hanya XL'
      ]},
      {name:'FLEX MANIA 15GB', price:43000, details:[
        'Kuota Utama 15GB Nasional',
        'Bisa diakumulasi dengan jenis/kode paket yang sama',
        'Bonus: Reguler 3GB / YouTube & TikTok 4.5GB / Kuota Malam 6GB (via MyXL)',
        'Tidak boleh ada jenis paket Flex lain (Unreg jika ada)',
        'Masa aktif 28 hari • Hanya XL'
      ]},
      {name:'FLEX MANIA 20GB', price:52000, details:[
        'Kuota Utama 20GB Nasional',
        'Bisa diakumulasi dengan jenis/kode paket yang sama',
        'Bonus: Reguler 5GB / YouTube & TikTok 7.5GB / Kuota Malam 10GB (via MyXL/*848#)',
        'Tidak boleh ada jenis paket Flex lain (Unreg jika ada)',
        'Masa aktif 28 hari • Hanya XL'
      ]},
      {name:'FLEX MANIA 25GB', price:61000, details:[
        'Kuota Utama 25GB Nasional',
        'Bisa diakumulasi dengan jenis/kode paket yang sama',
        'Bonus: Reguler 6GB / YouTube & TikTok 9GB / Kuota Malam 12GB (via MyXL/*848#)',
        'Tidak boleh ada jenis paket Flex lain (Unreg jika ada)',
        'Masa aktif 28 hari • Hanya XL'
      ]},
      {name:'FLEX MANIA 32GB', price:75000, details:[
        'Kuota Utama 32GB Nasional',
        'Bisa diakumulasi dengan jenis/kode paket yang sama',
        'Bonus: Reguler 8GB / YouTube & TikTok 12GB / Kuota Malam 16GB (via MyXL/*848#)',
        'Tidak boleh ada jenis paket Flex lain (Unreg jika ada)',
        'Masa aktif 28 hari • Hanya XL'
      ]},
    ];

    const masaAktif = [
      {name:'Perpanjang Masa Aktif AXIS 1 Bulan', price:10000},
      {name:'Perpanjang Masa Aktif XL 1 Bulan', price:10000},
      {name:'Perpanjang Masa Aktif TELKOMSEL 1 Bulan', price:12000},
    ];

    function openWA(text){ window.open(`https://wa.me/${ADMIN_WA_NUMBER}?text=${encodeURIComponent(text)}`, '_blank'); }
    function buildMessage(paketName, price){
      const nm=(document.getElementById('custName').value||'').trim();
      const no=(document.getElementById('custNumber').value||'').trim();
      return `Halo admin INNAH CELL,\nSaya ingin membeli: ${paketName}\nHarga: Rp ${price.toLocaleString('id-ID')}\n`+
             (nm?`Nama: ${nm}\n`:'') + (no?`Nomor Tujuan: ${no}\n`:'Nomor Tujuan: (belum diisi)\n') + 'Mohon diproses ya. Terima kasih.';
    }
    function buy(paketName, price){ openWA(buildMessage(paketName, price)); }
    function testChat(){ openWA('Halo admin, tes dari website.'); }

    function renderCircle(){
      const tb=document.getElementById('tbCircle');
      circlePackages.forEach(p=>{
        const tr=document.createElement('tr');
        tr.innerHTML=`<td>${p.name.replace('XL Circle ','')}</td>
                      <td>Rp ${p.price.toLocaleString('id-ID')}</td>
                      <td style='text-align:right'><button class='btn' onclick="buy('${p.name}', ${p.price})">Beli via WhatsApp</button></td>`;
        tb.appendChild(tr);
      });
    }
    function renderAkrab(){
      const list=document.getElementById('akrabList');
      akrabPackages.forEach(p=>{
        const el=document.createElement('div');
        el.className='card';
        el.innerHTML=`<div style='display:flex; justify-content:space-between; gap:12px; align-items:center'>
                        <div><div style='font-weight:800; font-size:1.05rem'>${p.name}</div>
                        <div class='muted'>Bisa ke XL / AXIS / LIVE ON</div></div>
                        <div style='text-align:right'>
                          <div style='font-weight:800'>Rp ${p.price.toLocaleString('id-ID')}</div>
                          <button class='btn' style='margin-top:8px' onclick="buy('${p.name}', ${p.price})">Beli via WhatsApp</button>
                        </div>
                      </div>`;
        list.appendChild(el);
      });
    }
    function renderFlex(){
      const list=document.getElementById('flexList');
      flexPackages.forEach(p=>{
        const el=document.createElement('div');
        el.innerHTML=`<details>
                        <summary>${p.name} • <span style='color:#bdf; font-weight:800'>Rp ${p.price.toLocaleString('id-ID')}</span></summary>
                        <ul class='muted' style='margin:10px 0 0 0'>${p.details.map(d=>`<li>${d}</li>`).join('')}</ul>
                        <div style='margin-top:12px; display:flex; gap:8px; align-items:center'>
                          <span class='pill'>Claim Bonus via MyXL / *848#</span>
                          <button class='btn' onclick="buy('${p.name}', ${p.price})">Beli via WhatsApp</button>
                        </div>
                      </details>`;
        list.appendChild(el);
      });
    }
    function renderMasaAktif(){
      const wrap=document.getElementById('masaAktif');
      masaAktif.forEach(p=>{
        const el=document.createElement('div');
        el.className='card';
        el.innerHTML=`<div style='display:flex; justify-content:space-between; gap:12px; align-items:center'>
                        <div style='font-weight:800'>${p.name}</div>
                        <div style='text-align:right'>
                          <div style='font-weight:800'>Rp ${p.price.toLocaleString('id-ID')}</div>
                          <button class='btn' style='margin-top:8px' onclick="buy('${p.name}', ${p.price})">Beli via WhatsApp</button>
                        </div>
                      </div>`;
        wrap.appendChild(el);
      });
    }

    // Background animation
    const bg=document.getElementById('bg-chart'); const ctx=bg.getContext('2d'); let W,H,t=0,lines=[];
    function resize(){const dpr=window.devicePixelRatio||1; W=bg.width=Math.floor(window.innerWidth*dpr); H=bg.height=Math.floor(window.innerHeight*dpr);
      bg.style.width=window.innerWidth+'px'; bg.style.height=window.innerHeight+'px'; ctx.setTransform(dpr,0,0,dpr,0,0); buildLines();}
    function buildLines(){lines=[]; const rows=4; for(let r=0;r<rows;r++){const baseY=(H/(rows+1))*(r+1); const col=r%2?'rgba(79,209,197,0.65)':'rgba(139,92,246,0.7)';
      lines.push({baseY,col,points:new Array(120).fill(0).map((_,i)=>({x:i*20,y:baseY+(Math.sin(i*.35+r)*12)}))});}}
    function draw(){ctx.clearRect(0,0,W,H); ctx.strokeStyle='rgba(255,255,255,0.05)'; ctx.lineWidth=1; const step=40;
      for(let x=0;x<W;x+=step){ctx.beginPath();ctx.moveTo(x,0);ctx.lineTo(x,H);ctx.stroke();}
      for(let y=0;y<H;y+=step){ctx.beginPath();ctx.moveTo(0,y);ctx.lineTo(W,y);ctx.stroke();}
      lines.forEach((ln,idx)=>{ln.points.forEach((p,i)=>{p.x-=1.2+idx*0.2; if(p.x<-40){p.x=W+40; p.y=ln.baseY+Math.sin((t+i)*0.2+idx)*(14+idx*3);}});
        ln.points.sort((a,b)=>a.x-b.x); ctx.beginPath(); for(let i=0;i<ln.points.length;i++){const p=ln.points[i]; if(i===0)ctx.moveTo(p.x,p.y); else ctx.lineTo(p.x,p.y);} ctx.strokeStyle=ln.col; ctx.lineWidth=2.5; ctx.stroke();});
      t+=0.016; requestAnimationFrame(draw);}
    function init(){document.getElementById('year').textContent=new Date().getFullYear();
      document.getElementById('waTop').href=`https://wa.me/6282301392317?text=${encodeURIComponent('Halo admin INNAH CELL, saya ingin membeli paket.')}`;
      renderCircle(); renderAkrab(); renderFlex(); renderMasaAktif(); resize(); draw(); window.addEventListener('resize', resize);}
    init();
  </script>
</body>
</html>
